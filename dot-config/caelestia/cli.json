{
  "toggles": {
    "communication": {
      "ferdium": {
        "enable": false,
        "match": [{ "class": "ferdium" }],
        "command": ["ferdium"],
        "move": true
      },
      "teams-work": {
        "enable": true,
        "match": [{ "class": "teams-work" }],
        "command": ["teams-for-linux-work-profile"],
        "move": true
      },
      "teams-school": {
        "enable": true,
        "match": [{ "class": "teams-school" }],
        "command": ["teams-for-linux-school-profile"],
        "move": true
      },
      "discord": {
        "enable": false
      },
      "whatsapp": {
        "enable": true,
        "match": [{ "class": "WhatsApp Desktop" }],
        "command": ["whatsapp-linux-desktop"],
        "move": true
      }
    },
    "email": {
      "thunderbird": {
        "enable": true,
        "match": [{ "class": "thunderbird" }],
        "command": ["thunderbird"],
        "move": true
      }
    }
  }
}
